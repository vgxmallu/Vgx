@Mbot.on_message(filters.private & filters.regex("ğŸ®PoKi Games") & pyro_cooldown.wait(10))
async def pokig(client, message):
    pk = await message.reply_text(
        text="**ğŸ•¹ï¸ Poki Games** [ã…¤](t.me/Msxg_bot/webpoki)", 
        reply_markup=ReplyKeyboardMarkup(
            [[
                "â—€ï¸ Go to Back", "âŒ CLOSE âŒ"
            ]], 
            resize_keyboard=True
        ) 
    ) 
    await asyncio.sleep(4)
    await message.delete()
    await asyncio.sleep(1800)
    await pk.delete()
    
